{
	"$schema": "https://railway.app/railway.schema.json",
	"build": {
		"builder": "NIXPACKS",
		"buildCommand": "npm run build"
	},
	"deploy": {
		"startCommand": "node scripts/deploy.js && node -r dotenv/config build/index.js",
		"restartPolicyType": "ON_FAILURE",
		"restartPolicyMaxRetries": 10
	}
}

